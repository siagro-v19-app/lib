<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		title="Motorista"
		class="sapUiPopupWithPadding"
		items="{/Motoristas}"
		search="_handleValueHelpSearch"
		confirm="_handleValueHelpClose"
		cancel="_handleValueHelpClose">
		<StandardListItem
			iconDensityAware="false"
			iconInset="false"
			title="{Nome}"
			description="{Id}" />
	</SelectDialog>
</core:FragmentDefinition>